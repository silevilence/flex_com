{
    "cmake.ignoreCMakeListsMissing": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.dart_tool": true,
        "**/build": true
    },
    "search.exclude": {
        "**/build": true,
        "**/.dart_tool": true,
        "**/*.g.dart": true, // 可选：搜索时忽略生成文件
        "**/*.freezed.dart": true // 可选
    },
    "files.watcherExclude": {
        "**/.dart_tool/**": true,
        "**/build/**": true
    }
}